<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="cds-metrics-collector">
    <Description/>
    <FaultRules/>
    <Flows/>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>KVM-GetGoogleOAuthClientId</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <HTTPTargetConnection>
        <Authentication>
            <GoogleIDToken>
                <Audience ref="private.GoogleOAuthClientId"/>
                <IncludeEmail>true</IncludeEmail>
            </GoogleIDToken>
        </Authentication>
        <LoadBalancer>
            <MaxFailures>0</MaxFailures>
            <RetryEnabled>true</RetryEnabled>
            <Server name="cds-metrics-collector">
                <IsEnabled>true</IsEnabled>
                <IsFallback>false</IsFallback>
                <Weight>1</Weight>
            </Server>
            <TargetDisableSecs>300</TargetDisableSecs>
        </LoadBalancer>
        <Path>/</Path>
        <Properties/>
    </HTTPTargetConnection>
</TargetEndpoint>