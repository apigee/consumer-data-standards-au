<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-SetResponseData">
    <DisplayName>AM-SetResponseData</DisplayName>
    <FaultRules/>
    <Properties/>
    <Set>
        <Payload contentType="application/json">{"data": {response.content}}</Payload>
    </Set>
