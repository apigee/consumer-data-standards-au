<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-CleanupHTMLResponse">
    <DisplayName>AM-CleanupHTMLResponse</DisplayName>
    <!-- The HTML document generated by the XSLT transform actually has xml content type (to prevent unclosed meta tags) -->
    <Properties/>
    <Set>
        <Headers>
            <Header name="content-type">text/html</Header>
        </Headers>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="response"/>
</AssignMessage>