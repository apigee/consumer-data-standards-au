<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-SetCurrentTimeAsEpochSeconds">
    <DisplayName>AM-SetCurrentTimeAsEpochSeconds</DisplayName>
    <Properties/>
    <AssignVariable>
        <Name>nowInEpochSeconds</Name>
        <Template>{substring(system.timestamp,0,10)}</Template>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>