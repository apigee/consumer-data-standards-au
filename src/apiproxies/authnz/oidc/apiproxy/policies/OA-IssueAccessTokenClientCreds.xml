<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OAuthV2 async="false" continueOnError="false" enabled="true" name="OA-IssueAccessTokenClientCreds">
    <DisplayName>OA-IssueAccessTokenClientCreds</DisplayName>
    <ExpiresIn>600000</ExpiresIn>
    <ExternalAuthorization>false</ExternalAuthorization>
    <GrantType>request.formparam.grant_type</GrantType>
    <Operation>GenerateAccessToken</Operation>
    <GenerateResponse enabled="true">
        <Format>FORM_PARAM</Format>
    </GenerateResponse>
    <Scope>request.formparam.scope</Scope>
    <SupportedGrantTypes>
        <GrantType>client_credentials</GrantType>
    </SupportedGrantTypes>
    <Attributes>
        <Attribute display="false" name="certFingerprint" ref="request.header.tls-client-cert-fingerprint"/>
    </Attributes>
</OAuthV2>