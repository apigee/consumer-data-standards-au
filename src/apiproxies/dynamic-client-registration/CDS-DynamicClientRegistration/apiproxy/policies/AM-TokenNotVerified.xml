<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-TokenNotVerified">
    <DisplayName>AM-TokenNotVerified</DisplayName>
    <FaultRules/>
    <Properties/>
    <Set>
        <Payload/>
        <StatusCode>401</StatusCode>
        <ReasonPhrase>Unauthorized</ReasonPhrase>
        <Headers>
            <Header name="WWW-Authenticate">error="invalid_token",error_description="{errorDetail}"</Header>
        </Headers>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo type="request" transport="http" createNew="false"/>
</AssignMessage>