<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="RF-InvalidContentType">
    <DisplayName>RF-InvalidContentType</DisplayName>
    <FaultResponse>
        <Set>
            <Payload contentType="application/json">{
 "error": "invalid_client_metadata",
 "error_description": "Content-type must be application/jwt"
}</Payload>
            <StatusCode>400</StatusCode>
            <ReasonPhrase>Bad Request</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <ShortFaultReason>false</ShortFaultReason>
</RaiseFault>