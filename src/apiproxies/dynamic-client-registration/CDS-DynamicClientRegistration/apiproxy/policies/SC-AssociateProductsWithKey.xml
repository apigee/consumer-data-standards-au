<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ServiceCallout async="false" continueOnError="false" enabled="true" name="SC-AssociateProductsWithKey">
    <DisplayName>SC-AssociateProductsWithKey</DisplayName>
    <Request clearPayload="true">
        <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
        <Set>
            <Headers>
                <Header name="Authorization">{private.apigeeAuth}</Header>
            </Headers>
            <Verb>POST</Verb>
            <Payload contentType="application/json"> {
  "apiProducts": {productList}
}
</Payload>
        </Set>
    </Request>
    <Response>apigeeAPIAddKeysResponse</Response>
    <HTTPTargetConnection>
        <URL>https://{apigeeAPIEndpoint}/v1/organizations/{organization.name}/developers/{ADRDetailsFromSSA.org_id}@ApigeeCDSRefImplementation.net/apps/{ADRDetailsFromSSA.software_id}/keys/{OIDCCredentials.client_id}</URL>
    </HTTPTargetConnection>
</ServiceCallout>