<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<GenerateJWT async="false" continueOnError="false" enabled="true" name="JWT-GeneratePARJWT">
    <DisplayName>JWT-GeneratePARJWT</DisplayName>
    <Algorithm>RS256</Algorithm>
    <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
    <PrivateKey>
        <Value ref="private.mockADRPrivateKey"/>
        <Id>CDSTestApp</Id>
    </PrivateKey>
    <Issuer ref="request.queryparam.client_id"/>
    <Subject ref="request.queryparam.client_id"/>
    <ExpiresIn>30m</ExpiresIn>
    <Id/>
    <Audience ref="audVar"/>
    <OutputVariable>theJWT</OutputVariable>
    <AdditionalClaims ref="additionalClaimsVar">
    </AdditionalClaims>
</GenerateJWT>