<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="false" enabled="true" name="EV-ExtractAccessTokenFromAuthHdr">
    <DisplayName>EV-ExtractAccessTokenFromAuthHdr</DisplayName>
    <Header name="Authorization">
        <Pattern ignoreCase="true">Bearer {theAccessToken}</Pattern>
    </Header>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <Source clearPayload="false">request</Source>
</ExtractVariables>