<SharedFlowBundle revision="1" name="manage-tokens-by-consent-id">
    <DisplayName>manage-tokens-by-consent-id</DisplayName>
    <Description>Keeps track of access and refresh tokens issued under a given consent id. Optionally revokes any existing tokens already associated with the consent id</Description>
    <CreatedAt>1635031590553</CreatedAt>
    <LastModifiedAt>1635031590553</LastModifiedAt>
    <Policies>
        <Policy>OA-RevokeAccessToken</Policy>
        <Policy>KVM-GetExistingTokens</Policy>
        <Policy>OA-RevokeRefreshToken</Policy>
        <Policy>KVM-PutNewOrUpdatedTokenInfo</Policy>
        <Policy>EV-ParseExistingTokensInfo</Policy>
        <Policy>AM-SetNewTokenInfo</Policy>
    </Policies>
    <SharedFlows>
        <SharedFlow>default</SharedFlow>
    </SharedFlows>
    <subType>SharedFlow</subType>
</SharedFlowBundle>