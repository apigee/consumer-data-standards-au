<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Quota type="flexi" async="false" continueOnError="true" enabled="true" name="Q-RequestsPerSessionForUnattendedAndAuthTraffic">
    <DisplayName>Q-RequestsPerSessionForUnattendedAndAuthTraffic</DisplayName>
    <FaultRules/>
    <Properties/>
    <Identifier ref="sessionId"/>
    <Allow count="100"/>
