    <Interval>1</Interval>
    <DataStoreFailureModeOpen>false</DataStoreFailureModeOpen>
    <Distributed>true</Distributed>
    <PreciseAtSecondsLevel>false</PreciseAtSecondsLevel>
    <Synchronous>false</Synchronous>
    <TimeUnit>day</TimeUnit>
</Quota>
