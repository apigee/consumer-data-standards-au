				{
					"name": "OIDC Acquire Token - Step 4 (Hybrid)",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "cda15dc4-ecab-452b-a4ad-0573bdad88c9",
								"exec": [
									"// Store scope, access token and id token to reuse in subsequent calls",
									"var jsonData = pm.response.json();",
									"pm.environment.set(\"access_token\", jsonData.access_token);",
									"pm.environment.set(\"id_token\", jsonData.id_token);",
									"pm.environment.set(\"refresh_token\", jsonData.refresh_token);",
									"pm.environment.set(\"scope\", jsonData.scope);",
									"",
									"// Store sub claim in id_token, so we can add it in x-cds-subject header in later requests",
									"var parts = jsonData.id_token.split('.'); // header, payload, signature",
									"var decodedJWTPayload = JSON.parse(atob(parts[1]));",
									"pm.environment.set(\"cdsSubject\",decodedJWTPayload.sub);",
									""
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"auth": {
							"type": "basic",
							"basic": [
								{
									"key": "password",
									"value": "{{client_secret}}",
									"type": "string"
								},
